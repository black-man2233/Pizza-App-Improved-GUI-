<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Page_Navigation_App.ViewModel"
                    xmlns:view="clr-namespace:Page_Navigation_App.View"
                    xmlns:local="clr-namespace:Page_Navigation_App">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomersVM}">
        <view:Customers/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:Products/>
    </DataTemplate>



</ResourceDictionary>